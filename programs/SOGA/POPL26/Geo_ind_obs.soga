x = uniform([-90, 90], 2);
y = uniform([-180, 180], 2);

r = gm([_p1, _p2], [_mu1, _mu2], [_sigma1, _sigma2]);

theta = uniform([0, 6.28], 2);

cosTheta = cos(theta);
xAnon = r*cosTheta;
xAnon = x + xAnon;

sinTheta = sin(theta);
yAnon = r*sinTheta;
yAnon = y + yAnon;

observe(xAnon==10.);
observe(yAnon==10.);